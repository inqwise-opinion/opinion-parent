<?xml version="1.0" encoding="UTF-8" ?>
<ruleset
    comparisonMethod=""
    xmlns="https://www.mojohaus.org/VERSIONS/RULE/2.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://www.mojohaus.org/VERSIONS/RULE/2.1.0 https://www.mojohaus.org/versions/versions-model/xsd/rule-2.1.0.xsd"
>
    <ignoreVersions>
        <!-- Ignore pre-release qualifiers -->
        <ignoreVersion type="regex">(?i).*-alpha.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-beta.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-rc.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-cr.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-milestone.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-m[0-9]+.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-preview.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-dev.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-snapshot.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[\\.-]pr[0-9]+.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[\\.-]b[0-9]+.*</ignoreVersion>
    </ignoreVersions>
    <rules>
    	<rule comparisonMethod="" groupId="io.vertx">
    		<ignoreVersions>
				<ignoreVersion type="regex">(?i).*-milestone(?:-?\d+)?</ignoreVersion>
			</ignoreVersions>
     	</rule>
     	<rule comparisonMethod="" groupId="io.vertx" artifactId="vertx-docgen">
    		<ignoreVersions>
				<ignoreVersion type="regex">.*</ignoreVersion>
			</ignoreVersions>
     	</rule>
     	<rule comparisonMethod="" groupId="com.fasterxml.jackson">
    		<ignoreVersions>
				<ignoreVersion type="regex">(?i).*\.pr1</ignoreVersion>
			</ignoreVersions>
     	</rule>
     	<rule comparisonMethod="" groupId="io.netty">
    		<ignoreVersions>
				<ignoreVersion type="regex">.*</ignoreVersion>
			</ignoreVersions>
     	</rule>
     	<rule comparisonMethod="" groupId="javax.xml.bind">
    		<ignoreVersions>
				<ignoreVersion type="regex">(?i).*-b([0-9\.]+)</ignoreVersion>
			</ignoreVersions>
     	</rule>
     	<rule comparisonMethod="" groupId="org.infinispan">
    		<ignoreVersions>
				<ignoreVersion type="regex">(?i).*\.Dev([0-9]+)?</ignoreVersion>
			</ignoreVersions>
     	</rule>
    </rules>
</ruleset>
